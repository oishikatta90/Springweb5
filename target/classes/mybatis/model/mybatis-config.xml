<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<typeAlias type="com.spring.web5.vo.CustomerVO" alias="customer" />
		<typeAlias type="com.spring.web5.vo.BoardVO" alias="board" />
		<typeAlias type="com.spring.web5.vo.ReplyVO" alias="reply" />
	</typeAliases>
	
	<mappers>
		<mapper resource="mybatis/mappers/customerMapper.xml" />
		<mapper resource="mybatis/mappers/boardMapper.xml" />
	</mappers>

</configuration>